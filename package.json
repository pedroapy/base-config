{
    "name": "@pedroapy/base-config",
    "version": "0.0.0-development",
    "description": "This is my basic configurations for development.",
    "keywords": [
        "eslint",
        "eslint-config"
    ],
    "author": "@pedroapy",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pedroapy/base-config.git"
