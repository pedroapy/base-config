    },
    "devDependencies": {
        "vite": "5.1.5",
        "vitest": "1.3.1"
    },
    "peerDependencies": {
        "vite": "^5.0.0",
        "vitest": "^1.0.0"
    },
    "dependencies": {
        "@vitejs/plugin-react": "4.2.1",
        "@vitest/coverage-v8": "1.3.1",
        "@vitest/ui": "1.3.1",
        "jsdom": "24.0.0",
        "vite-tsconfig-paths": "4.3.1"
    }
}
