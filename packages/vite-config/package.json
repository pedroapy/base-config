{
    "name": "@pedroapy/vite-config",
    "version": "1.5.1",
    "description": "The @pedroapy shared vite config",
    "keywords": [
        "vite"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pedroapy/base-config.git"
    },
    "author": "@pedroapy",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
