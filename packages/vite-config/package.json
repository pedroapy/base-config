    },
    "peerDependencies": {
        "vite": "^4.4.7",
        "vitest": "^0.33.0"
    },
    "dependencies": {
        "@vitejs/plugin-react": "4.0.3",
        "@vitest/coverage-v8": "0.33.0",
        "@vitest/ui": "0.33.0",
        "jsdom": "22.1.0",
        "vite-tsconfig-paths": "4.2.0"
    }
